# Use GNU C/C++ compiler by default
CC=gcc
CXX=g++
# We assume that yacc supports the -o option; bison and byacc do.
YACC=yacc -d
LEX=lex

#CFLAGS=-Wall -O2 -DVERSION='"0.5"' -g -pg
CFLAGS=-Wall -O2 -DVERSION='"0.5"' -DNDEBUG -DIC3_SUPPRESS_STD_CERR
#CXXFLAGS=-Wall -O2 -std=c++11 -g -pg
CXXFLAGS=-Wall -O2 -std=c++11 -DNDEBUG -DIC3_SUPPRESS_STD_CERR
CPPFLAGS=
#LDFLAGS=-pg
LDFLAGS=
LIBS=

export CC CXX LEX YACC CFLAGS CXXFLAGS CPPFLAGS LDFLAGS LIBS

