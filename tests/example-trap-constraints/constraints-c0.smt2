(declare-fun nc () Int)
(declare-fun cs1 () Int)
(declare-fun cs2 () Int)
(declare-fun s1 () Int)
(declare-fun s2 () Int)
(declare-fun t1 () Int)
(declare-fun t2 () Int)
(declare-fun t3 () Int)
(declare-fun t4 () Int)
(assert (>= nc 0))
(assert (>= cs1 0))
(assert (>= cs2 0))
(assert (>= s1 0))
(assert (>= s2 0))
(assert (>= t1 0))
(assert (>= t2 0))
(assert (>= t3 0))
(assert (>= t4 0))
(assert (= nc (+ 2 (- t1) (- t2) t3 t4)))
